{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "r2-share",
	"main": "src/index.js",
	"compatibility_date": "2025-03-03",
	"observability": {
		"enabled": true
	},
  "r2_buckets": [
    {
      "binding": "FILE_BUCKET",
      "bucket_name": "r2-share"
    }
  ],
  "routes": [
    {
      "pattern": "r2-share.jdores.xyz",
      "custom_domain": true
    }
  ],
  "workers_dev": false,
  "preview_urls": false
}